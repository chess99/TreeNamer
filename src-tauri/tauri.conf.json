{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "treenamer",
  "version": "0.1.0",
  "identifier": "com.treenamer.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devPath": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "distDir": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "treenamer",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "identifier": "com.treenamer.app",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.treenamer.app",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "TreeNamer",
        "width": 1024,
        "height": 768
      }
    ],
    "plugins": {
      "dialog": {}
    }
  }
}
